{
  "name": "modifyljf/laravel-clover",
  "description": "Clover APIs PHP(Laravel) Helper Library wrote by Jianfeng Li.",
  "keywords": [
    "Clover",
    "laravel"
  ],
  "license": "MIT",
  "homepage": "https://github.com/modifyljf/laravel-clover",
  "support": {
    "issues": "https://github.com/modifyljf/laravel-clover/issues",
    "source": "https://github.com/modifyljf/laravel-clover"
  },
  "authors": [
    {
      "name": "Jianfeng Li",
      "email": "modifyljf@gmail.com"
    }
  ],
  "require": {
    "php": "~8.0",
    "ext-json": "*",
    "laravel/framework": "~8.0"
  },
  "require-dev": {
    "facade/ignition": "^2.5",
    "fakerphp/faker": "^1.9.1",
    "laravel/sail": "^1.0.1",
    "mockery/mockery": "^1.4.4",
    "nunomaduro/collision": "^5.10",
    "phpunit/phpunit": "^9.5.10"
  },
  "autoload": {
    "psr-4": {
      "Modifyljf\\Clover\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Modifyljf\\Clover\\Providers\\CloverServiceProvider"
      ],
      "aliases": {
      }
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
